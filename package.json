{
  "name": "jigswap-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "nx run-many --target=dev --projects=web",
    "dev:web": "nx dev web",
    "build": "nx run-many --target=build --all",
    "build:web": "nx build web",
    "start": "nx run-many --target=start --projects=web",
    "start:web": "nx start web",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all -- --fix",
    "type-check": "nx run-many --target=type-check --all",
    "test": "nx run-many --target=test --all",
    "clean": "nx reset && rm -rf apps/*/dist apps/*/.next packages/*/dist",
    "format": "prettier --write . || true",
    "format:check": "prettier --check . || true",
    "graph": "nx graph",
    "affected": "nx affected"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@nx/eslint": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/next": "21.3.11",
    "@nx/vite": "21.3.11",
    "@nx/web": "21.3.11",
    "@swc-node/core": "^1.13.3",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "~1.13.3",
    "@swc/helpers": "~0.5.17",
    "@types/node": "^24.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "jiti": "2.5.1",
    "jsonc-eslint-parser": "^2.4.0",
    "nx": "21.3.11",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "5.9.2",
    "vite": "^7.0.7",
    "vitest": "^3.2.4"
  },
  "nx": {
    "includedScripts": []
  }
}
