{
  "name": "jigswap-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "nx run-many --target=dev --projects=web",
    "dev:web": "nx dev web",
    "build": "nx run-many --target=build --all",
    "build:web": "nx build web",
    "start": "nx run-many --target=start --projects=web",
    "start:web": "nx start web",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all -- --fix",
    "type-check": "nx run-many --target=type-check --all",
    "test": "nx run-many --target=test --all",
    "clean": "nx reset && rm -rf apps/*/dist apps/*/.next packages/*/dist",
    "format": "prettier --write . || true",
    "format:check": "prettier --check . || true",
    "graph": "nx graph",
    "affected": "nx affected"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@nx/eslint": "22.2.0",
    "@nx/js": "22.2.0",
    "@nx/next": "22.2.0",
    "@nx/vite": "22.2.0",
    "@nx/web": "22.2.0",
    "@swc-node/core": "^1.14.1",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "~1.15.3",
    "@swc/helpers": "~0.5.17",
    "@types/node": "^24.10.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9.39.1",
    "jiti": "2.6.1",
    "jsonc-eslint-parser": "^2.4.2",
    "nx": "22.2.0",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tslib": "^2.8.1",
    "typescript": "5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "nx": {
    "includedScripts": []
  }
}
